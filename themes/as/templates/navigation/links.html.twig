{#
#}
{% if links -%}
    {%- if heading -%}
        {%- if heading.level -%}
            <{{ heading.level }}{{ heading.attributes }}>{{ heading.text }}</{{ heading.level }}>
        {%- else -%}
            <h2{{ heading.attributes }}>{{ heading.text }}</h2>
        {%- endif -%}
    {%- endif -%}
    <ul{{ attributes }} style="text-align:right">
        {%- for item in links -%}
            <li{{ item.attributes }} style='list-style-type: none'>
                {%- if item.link -%}
                    {{ item.link }}
                {%- elseif item.text_attributes -%}
                    <span{{ item.text_attributes }}>{{ item.text }}</span>
                {%- else -%}
                    {{ item.text }}
                {%- endif -%}
            </li>
        {%- endfor -%}
    </ul>
{%- endif %}
